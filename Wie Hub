local Library = loadstring(game:HttpGet("https://raw.githubusercontent.com/xHeptc/Kavo-UI-Library/main/source.lua"))()  

function Script()  
    local Window = Library.CreateLib("Porno Hub", "Synapse")  

    local Main = Window:NewTab("Main")  
    local MainSection = Main:NewSection("Main")  

    -- Script  

    MainSection:NewSlider("Yürüyüş Hızı", "Yürüme Hızınızı Değiştirebilirsiniz", 250, 16, function(s) -- 500 (MaxValue) | 0 (MinValue)  
        game.Players.LocalPlayer.Character.Humanoid.WalkSpeed = s  
    end)  

    MainSection:NewSlider("Zıplama Gücü", "Ne Kadar Güçte Zıplamak İsterseniz O Kadar Zıplar", 250, 16, function(s) -- 500 (MaxValue) | 0 (MinValue)  
        game.Players.LocalPlayer.Character.Humanoid.JumpPower = s  
    end)  

    MainSection:NewButton("Adminlik", "Bütün Admin Komutlarını Bedava Kullanabilirsiniz", function()  
        loadstring(game:HttpGet('https://raw.githubusercontent.com/FilteringEnabled/NamelessAdmin/main/Source'))()  
    end)  

    MainSection:NewToggle("Sınırsız Zıplamak", "Zıplama Tuşuna Basmayı Bırakana Kadar Havaya Zıplar", function()  
        local InfiniteJumpEnabled = true  
        game:GetService("UserInputService").JumpRequest:connect(function()  
            if InfiniteJumpEnabled then  
                game:GetService("Players").LocalPlayer.Character:FindFirstChildOfClass('Humanoid'):ChangeState("Jumping")  
            end  
        end)  
    end)  

    local Credits = Window:NewTab("Oyun Scriptleri")  
    local CreditsSection = Credits:NewSection("Oyun Scriptleri")  

    CreditsSection:NewButton("Murder Mystery 2", "Murder Mystery 2 İçin Script ", function()  
        loadstring(game:HttpGet("https://raw.githubusercontent.com/13XIII3III/EclipseHub/main/EclipseHubMM2.lua", true))()  
    end)
end
-- Scripti her oyunda çalıştır Local Yarramı Ye
Script()
